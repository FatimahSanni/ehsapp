<%= simple_form_for(@procedure_record) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :patient %>
  </div>

  <div class="form-inputs">
  	<%= f.fields_for :procedures do |p| %>
  	<%= render 'procedure_fields', f:p %>
  	<% end %>
  	<%= link_to_add_association 'Add Procedure', f, :procedures %>
  </div>

  <div class="form-inputs">
  	<%= f.association :consultant %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

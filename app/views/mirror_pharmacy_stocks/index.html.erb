<p id="notice"><%= notice %></p>

<h1>Listing Mirror Pharmacy Stocks</h1>

<table>
  <thead>
    <tr>
      <th>Amount</th>
      <th>Pharmacy stock</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mirror_pharmacy_stocks.each do |mirror_pharmacy_stock| %>
      <tr>
        <td><%= mirror_pharmacy_stock.amount %></td>
        <td><%= mirror_pharmacy_stock.pharmacy_stock %></td>
        <td><%= link_to 'Show', mirror_pharmacy_stock %></td>
        <td><%= link_to 'Edit', edit_mirror_pharmacy_stock_path(mirror_pharmacy_stock) %></td>
        <td><%= link_to 'Destroy', mirror_pharmacy_stock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mirror pharmacy stock', new_mirror_pharmacy_stock_path %>

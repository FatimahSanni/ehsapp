<p id="notice"><%= notice %></p>

<h1>Listing Pharmacy Stocks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Amount</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pharmacy_stocks.each do |pharmacy_stock| %>
      <tr>
        <td><%= pharmacy_stock.name %></td>
        <td><%= pharmacy_stock.amount %></td>
        <td><%= pharmacy_stock.category %></td>
        <td><%= link_to 'Show', pharmacy_stock %></td>
        <td><%= link_to 'Edit', edit_pharmacy_stock_path(pharmacy_stock) %></td>
        <td><%= link_to 'Destroy', pharmacy_stock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pharmacy stock', new_pharmacy_stock_path %>
